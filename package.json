{
  "name": "frontend",
  "version": "1.0.0",
  "private": true,
  "type": "module",
  "scripts": {
    "start": "rimraf dist && rsbuild dev",
    "dev": "rimraf dist && rsbuild dev",
    "build": "rimraf dist && rsbuild build",
    "build:prod": "npm run build",
    "lint": "npm run tsc:check && npm run eslint:check && npm run prettier:check",
    "lint:fix": "npm run eslint:fix && npm run prettier:fix",
    "eslint:check": "eslint \"src/**/*.{js,ts,tsx}\" --quiet",
    "eslint:fix": "eslint \"src/**/*.{js,ts,tsx}\" --quiet --fix",
    "prettier:check": "prettier --check .",
    "prettier:fix": "prettier --write .",
    "tsc:check": "tsc --noEmit",
    "prune": "ts-prune",
    "tsr:check": "tsr src/index.tsx",
    "tsr:clean": "tsr --write src/index.tsx",
    "tsr:clean:recursive": "tsr --recursive --write src/index.tsx",
    "cleanup": "npm run lint && npm run tsr:clean",
    "security:check": "npm audit --registry=https://registry.npmjs.org --audit-level=critical",
    "security:fix": "npm audit fix --registry=https://registry.npmjs.org && rimraf ./node_modules && npm cache clear --force && npm install",
    "prepare": "husky"
  },
  "dependencies": {
    "@tanstack/react-query": "^5.59.6",
    "@tanstack/react-query-devtools": "^5.59.6",
    "clsx": "^2.1.1",
    "dayjs": "^1.11.13",
    "react": "^19.2.0",
    "react-dnd": "^16.0.1",
    "react-dnd-html5-backend": "^16.0.1",
    "react-dom": "^19.2.0",
    "react-router-dom": "^6.28.0",
    "rimraf": "^6.1.0"
  },
  "devDependencies": {
    "@rsbuild/core": "^1.6.0",
    "@rsbuild/plugin-react": "^1.4.1",
    "@types/react": "^19.2.2",
    "@types/react-dom": "^19.2.2",
    "@typescript-eslint/eslint-plugin": "^8.47.0",
    "@typescript-eslint/parser": "^8.47.0",
    "eslint": "^9.39.1",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-import": "^2.32.0",
    "eslint-plugin-jsx-a11y": "^6.10.2",
    "eslint-plugin-prettier": "^5.5.4",
    "eslint-plugin-react": "^7.37.5",
    "eslint-plugin-react-hooks": "^7.0.1",
    "husky": "^9.1.7",
    "prettier": "^3.6.2",
    "typescript": "^5.9.3"
  }
}
